{
    "watch": ["src"],
    "ext": ".ts,.js",
    "ignore": [],
    "exec": "node --inspect -r ts-node/register ./src/server.js | pino-pretty",
    "env": {
      "LOCAL_S3": "true",
      "STRIPE_LIVE": "NO",
      "NO_EMAIL": "true",
      "ENV": "dev",
      "PUBLIC_URL": "http://localhost:3001"
    },
    "inspect": "true"
  }